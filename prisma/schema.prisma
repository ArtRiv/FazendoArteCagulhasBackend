generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  created_at      Int
  description     String[]
  id              String @id @default(uuid())
  image           String
  link            String
  price           Int
  product_group   String
  purchase_count  Int
  tag             String
  title           String
  type            String
}
